<?xml version="1.0"?>
<PropertyList>
	<generic>
		<input>
			<line_separator>\n</line_separator>
			<var_separator>,</var_separator>
			<binary_mode>false</binary_mode>
		</input>
		<output>
			<line_separator>\n</line_separator>
			<var_separator>,</var_separator>
			<binary_mode>false</binary_mode>
			<chunk>
				<name>roll</name>
				<type>double</type>
				<node>/orientation/roll-deg</node>
				<format>{"roll":"%f"</format>
			</chunk>
			<chunk>
				<name>pitch</name>
				<type>double</type>
				<node>/orientation/pitch-deg</node>
				<format>"pitch":"%f"</format>
			</chunk>
			<chunk>
				<name>heading</name>
				<type>double</type>
				<node>/orientation/heading-deg</node>
				<format>"heading":"%f"</format>
			</chunk>
			<chunk>
				<name>aoa</name>
				<type>double</type>
				<node>/orientation/alpha-deg</node>
				<format>"aoa":"%f"</format>
			</chunk>
			<chunk>
				<name>sideslip</name>
				<type>double</type>
				<node>/orientation/side-slip-deg</node>
				<format>"sideslip":"%f"}</format>
			</chunk>
		</output>
	</generic>
</PropertyList>